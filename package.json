{
    "name": "todo-desktop-app",
    "version": "1.0.0",
    "description": "A simple To-Do desktop application",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "pack": "electron-builder --dir",
      "dist": "electron-builder --win portable"
    },
    "keywords": [
      "todo",
      "electron",
      "desktop",
      "productivity"
    ],
    "author": "",
    "license": "MIT",
    "build": {
      "appId": "com.yourname.todo",
      "productName": "To-Do",
      "directories": {
        "output": "dist"
      }
    },
    "devDependencies": {
      "electron": "^29.0.0",
      "electron-builder": "^24.6.4"
    },
    "dependencies": {
      "@fortawesome/fontawesome-free": "^6.4.0"
    }
  }